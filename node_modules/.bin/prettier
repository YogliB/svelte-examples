#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/Projects/svelte-examples/node_modules/.pnpm/prettier@2.2.1/node_modules/prettier/bin-prettier.js/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/prettier@2.2.1/node_modules/prettier/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/prettier@2.2.1/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/node_modules:/mnt/d/Projects/svelte-examples/node_modules:/mnt/d/Projects/node_modules:/mnt/d/node_modules:/mnt/d/Projects/svelte-examples/node_modules/prettier/bin-prettier.js/node_modules:/mnt/d/Projects/svelte-examples/node_modules/prettier/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prettier/bin-prettier.js" "$@"
else 
  exec node  "$basedir/../prettier/bin-prettier.js" "$@"
fi
