#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/Projects/svelte-examples/node_modules/.pnpm/rollup@2.39.0/node_modules/rollup/dist/bin/rollup/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/rollup@2.39.0/node_modules/rollup/dist/bin/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/rollup@2.39.0/node_modules/rollup/dist/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/rollup@2.39.0/node_modules/rollup/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/rollup@2.39.0/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/node_modules:/mnt/d/Projects/svelte-examples/node_modules:/mnt/d/Projects/node_modules:/mnt/d/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/@rollup/plugin-commonjs@17.1.0_rollup@2.39.0/node_modules/@rollup/plugin-commonjs/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/@rollup/plugin-commonjs@17.1.0_rollup@2.39.0/node_modules/@rollup/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/@rollup/plugin-commonjs@17.1.0_rollup@2.39.0/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/@rollup/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../rollup@2.39.0/node_modules/rollup/dist/bin/rollup" "$@"
else 
  exec node  "$basedir/../../../../../../../rollup@2.39.0/node_modules/rollup/dist/bin/rollup" "$@"
fi
