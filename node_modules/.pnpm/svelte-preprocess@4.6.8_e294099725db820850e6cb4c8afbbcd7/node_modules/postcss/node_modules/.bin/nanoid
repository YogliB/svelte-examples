#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/Projects/svelte-examples/node_modules/.pnpm/nanoid@3.1.20/node_modules/nanoid/bin/nanoid.cjs/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/nanoid@3.1.20/node_modules/nanoid/bin/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/nanoid@3.1.20/node_modules/nanoid/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/nanoid@3.1.20/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/node_modules:/mnt/d/Projects/svelte-examples/node_modules:/mnt/d/Projects/node_modules:/mnt/d/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/postcss@8.2.6/node_modules/postcss/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/postcss@8.2.6/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nanoid@3.1.20/node_modules/nanoid/bin/nanoid.cjs" "$@"
else 
  exec node  "$basedir/../../../../../nanoid@3.1.20/node_modules/nanoid/bin/nanoid.cjs" "$@"
fi
