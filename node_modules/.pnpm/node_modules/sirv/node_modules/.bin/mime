#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/Projects/svelte-examples/node_modules/.pnpm/mime@2.5.0/node_modules/mime/cli.js/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/mime@2.5.0/node_modules/mime/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/mime@2.5.0/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/node_modules:/mnt/d/Projects/svelte-examples/node_modules:/mnt/d/Projects/node_modules:/mnt/d/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/sirv@1.0.11/node_modules/sirv/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/sirv@1.0.11/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mime@2.5.0/node_modules/mime/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../mime@2.5.0/node_modules/mime/cli.js" "$@"
fi
