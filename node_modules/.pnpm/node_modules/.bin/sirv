#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/Projects/svelte-examples/node_modules/.pnpm/sirv-cli@1.0.11/node_modules/sirv-cli/bin.js/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/sirv-cli@1.0.11/node_modules/sirv-cli/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/sirv-cli@1.0.11/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/node_modules:/mnt/d/Projects/svelte-examples/node_modules:/mnt/d/Projects/node_modules:/mnt/d/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/node_modules/sirv-cli/bin.js/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/node_modules/sirv-cli/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sirv-cli/bin.js" "$@"
else 
  exec node  "$basedir/../sirv-cli/bin.js" "$@"
fi
