#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/Projects/svelte-examples/node_modules/.pnpm/sass@1.32.7/node_modules/sass/sass.js/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/sass@1.32.7/node_modules/sass/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/sass@1.32.7/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/node_modules:/mnt/d/Projects/svelte-examples/node_modules:/mnt/d/Projects/node_modules:/mnt/d/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/node_modules/sass/sass.js/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/node_modules/sass/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sass/sass.js" "$@"
else 
  exec node  "$basedir/../sass/sass.js" "$@"
fi
