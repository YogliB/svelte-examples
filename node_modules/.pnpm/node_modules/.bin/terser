#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/d/Projects/svelte-examples/node_modules/.pnpm/terser@5.6.0/node_modules/terser/bin/terser/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/terser@5.6.0/node_modules/terser/bin/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/terser@5.6.0/node_modules/terser/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/terser@5.6.0/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/node_modules:/mnt/d/Projects/svelte-examples/node_modules:/mnt/d/Projects/node_modules:/mnt/d/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/node_modules/terser/bin/terser/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/node_modules/terser/bin/node_modules:/mnt/d/Projects/svelte-examples/node_modules/.pnpm/node_modules/terser/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../terser/bin/terser" "$@"
else 
  exec node  "$basedir/../terser/bin/terser" "$@"
fi
